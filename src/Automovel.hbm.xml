<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "
-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>	
		
	<class name="locadora.model.automovel.Automovel">
		<id name="id" type="java.lang.Long">
			<generator class="increment"></generator>
		</id>	
		
		<property name="ano"/>	
		<property name="capacidade"/>
		<property name="chassi"/>
		<property name="codCategoria"/>
		<property name="consumo"/>		
		<property name="cor"></property>
		<property name="modelo"></property>
		<property name="placa"></property>
		<property name="preco"></property>
		<property name="status"></property>				

	 <!--  <many-to-one name="locadora.model.adicional.Adicional" class="Adicional" column="id" cascade="none">		
	  </many-to-one>  --> 
		
	</class>

</hibernate-mapping>