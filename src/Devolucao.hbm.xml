<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="locadora.model.Devolucao">
	
	
		<id name="id" column="automovel_id">
			<generator class="foreign">
				<param name="property">reserva</param>
			</generator>			
		</id>	
	
		<many-to-one name="funcionario" class="locadora.model.Funcionario" column="funcionario_id"></many-to-one>
		
		<one-to-one name="reserva" class="locadora.model.Reserva" constrained="true"> </one-to-one>
	
	
	</class>
</hibernate-mapping>



  