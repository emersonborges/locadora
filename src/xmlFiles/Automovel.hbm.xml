<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "
-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>	
		
	<class name="Automovel">
		<id column="" name="id" type="java.lang.Long">
			<generator class="increment"></generator>
		</id>	
		
		<property name="ano"/>	
		<property name="capacidade"/>
		<property name="chassi"/>
		<property name="codCategoria"/>
		<property name="consumo"/>		
		<property name="cor"></property>
		<property name="modelo"></property>
		<property name="placa"></property>
		<property name="preco"></property>	
		
	</class>

</hibernate-mapping>